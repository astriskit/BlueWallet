{
  "expo": {
    "name": "BlueWallet(Expo)",
    "slug": "bluewallet",
    "scheme": "bluewallet",
    "newArchEnabled": true,
    "icon": "./src/img/icon.png",
    "android": {
      "package": "com.harry_expo.bluewallet",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.harry-expo.bluewallet",
      "infoPlist": {
        "NSCameraUsageDescription": "In order to quickly scan the recipient's address, we need your permission to use the camera to scan their QR Code."
      },
      "entitlements": {
        "aps-environment": "development"
      }
    },
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": [
      "expo-router",
      "expo-quick-actions",
      [
        "expo-splash-screen",
        {
          "image": "./src/img/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-permissions",
        {
          "iosPermissions": [
            "Camera",
            "Notifications"
          ]
        }
      ],
      "@config-plugins/detox"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5902ca6c-31e8-4737-a286-e93dba1150c2"
      }
    }
  }
}
